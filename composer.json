{
    "name": "metrique/laravel-plonk",
    "description": "Library for image management in Laravel 5.",
    "type": "Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hudson",
            "email": "support@metrique.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Metrique\\Plonk\\": "src/"
